generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model DemoRequest {
  id         Int      @id @default(autoincrement())
  email      String   @unique
  name       String
  company    String?
  prompt     String
  previewUrl String
  createdAt  DateTime @default(now())
}
